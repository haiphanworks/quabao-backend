{
  "kind": "collectionType",
  "collectionName": "tickets",
  "info": {
    "name": "ticket",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "price": {
      "type": "integer"
    },
    "users": {
      "via": "ticket",
      "plugin": "users-permissions",
      "collection": "user"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "description": {
      "type": "richtext"
    },
    "order": {
      "via": "ticket",
      "model": "order"
    },
    "session": {
      "type": "enumeration",
      "enum": [
        "day",
        "night",
        "vip"
      ]
    }
  }
}
